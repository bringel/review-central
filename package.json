{
  "name": "github-review-central",
  "description": "A github pull request review dashboard",
  "version": "1.0.0",
  "author": "Brad Ringel <brad.ringel@gmail.com>",
  "private": true,
  "scripts": {
    "devServer": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "apiServer": "node server/server.js",
    "dev": "npm-run-all -p devServer apiServer",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "copyIndex": "cp index.html dist/index.html",
    "start": "npm run build && cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "oauth": "^0.9.15",
    "vue": "^2.3.3",
    "vue-resource": "^1.3.4"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^5.0.1",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
