{
  "name": "github-review-central",
  "description": "A github pull request review dashboard",
  "version": "1.0.0",
  "author": "Brad Ringel <brad.ringel@gmail.com>",
  "private": true,
  "scripts": {
    "devServer": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "apiServer": "nodemon --watch server --exec ./node_modules/.bin/babel-node server/server.js",
    "dev": "npm-run-all -p devServer apiServer",
    "buildApp": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "buildServer": "rimraf server/dist && ./node_modules/.bin/babel server -d server/dist --copy-files ",
    "start": "npm-run-all buildApp buildServer && cross-env NODE_ENV=production node server/dist/server.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "oauth": "^0.9.15",
    "pg": "^6.4.2",
    "rimraf": "^2.6.2",
    "sequelize": "^4.28.0",
    "vue": "^2.3.3",
    "vue-resource": "^1.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^4.14.0",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^5.0.1",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
